<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.board.mapper.BoardMapper">
  <select id="getBoardList">
  SELECT  BNO,
          MENU_ID,
		  TITLE,
		  CONTENT,
		  WRITER,
		  REGDATE,
		  HIT
  FROM    BOARD
 ORDER BY BNO DESC
  </select>
  
</mapper>